@inproceedings{ ISI:000087021500023,
Author = {Timmerman, A and Jacques, D and Feyen, J},
Editor = {{Feyen, J and Wiyo, K}},
Title = {{Determination of soil hydraulic parameters combining outflow and suction
   crust infiltrometer experiments}},
Booktitle = {{MODELLING OF TRANSPORT PROCESSES IN SOILS: AT VARIOUS SCALES IN TIME AND
   SPACE}},
Year = {{1999}},
Pages = {{215-224}},
Note = {{International Workshop of the European-Society-of-Agricultural-Engineers
   Field of Interest on Soil and Water, LEUVEN, BELGIUM, NOV 24-26, 1999}},
Organization = {{European Soc Agr Engineers, Soil \& Water Interest Grp; CIGR; ICID;
   CIID; CNR; Katholieke Univ Leuven, Fac Agr \& Appl Biol Sci; Inst Land
   \& Water Management}},
Abstract = {{To derive soil hydraulic information, transient outflow experiments are
   an increasingly better tool to direct measurements of soil hydraulic
   properties. The accuracy of these procedures to simulate other variables
   from outflow data has however been questioned. Therefore, we measured
   four other auxiliary variables from among pressure heads at two
   locations and water content at one location as a function of time and
   dynamic water retention data during multistep outflow experiments.
   Independently, unsaturated conductivity data was measured with the
   suction crust infiltrometer method. These data served as input for
   different combinations of objective functions. It was the objective of
   this study to experimentally explore the feasibility of different types
   of objective functions to simulate measured variables. The addition of
   specific information of a variable into the objective function results
   in better parameter estimates to simulate that variable brit this was no
   guarantee that other measured variables were simulated as well. However,
   a final parameter that describes all measured variables accurately was
   not found.}},
