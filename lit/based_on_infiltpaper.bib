@article{ ISI:000254960900023,
Author = {Looms, Majken C. and Jensen, Karsten H. and Binley, Andrew and Nielsen,
   Lars},
Title = {{Monitoring unsaturated flow and transport using cross-borehole
   geophysical methods}},
