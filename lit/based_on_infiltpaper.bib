@article{ ISI:000300767000022,
Author = {Vignoli, Giulio and Deiana, Rita and Cassiani, Giorgio},
Title = {{Focused inversion of vertical radar profile (VRP) traveltime data}},
Journal = {{GEOPHYSICS}},
