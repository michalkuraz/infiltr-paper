@article{ ISI:000275009400004,
Author = {Gnatowski, Tomasz and Szatylowicz, Jan and Brandyk, Tomasz and
   Kechavarzi, Cedric},
Title = {{Hydraulic properties of fen peat soils in Poland}},
Journal = {{GEODERMA}},
Year = {{2010}},
